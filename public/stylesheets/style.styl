@import '//cdnjs.cloudflare.com/ajax/libs/twitter-bootstrap/3.1.1/css/bootstrap.min.css'
@import 'http://stockickr.com/news/css/screen.css'
@import 'http://stockickr.com/g/4a6e868cfce70f/stylesheets/f/fonts.css'
.atomic-bomb-on-the-icon
  -webkit-transition background-image cubic-bezier(0.250, 0.460, 0.450, 0.940) .5s
  -moz-transition background-image cubic-bezier(0.250, 0.460, 0.450, 0.940) .5s
  -o-transition background-image cubic-bezier(0.250, 0.460, 0.450, 0.940) .5s
  transition background-image cubic-bezier(0.250, 0.460, 0.450, 0.940) .5s
//fullBleedThing
.sitewrap:before, .sitewrap:after
  content ""
  display table
  min-height 100%
.sitewrap
  background #ffff repeat scroll 1 0 0
  &:after
    clear both
.main-nav
  z-index 1
  position fixed
  float left
  background-color rgba(255,255,255,0.01)
  opacity .99
  padding-left 2%
  padding-top 2%
  height auto
  width auto
#tiles
  margin 50px
  li
    width 400px
    height 200px
    margin 20em auto
    background #333
#header
  background url('http://s3.amazonaws.com/stockickr/assets/images/bg/greentessellation.jpg') center center fixed
  background-size cover
  background-position left top
  height 100%
  width 100%
  min-height 100%
  text-align center
.header-container  
  text-align center
  min-width 100%
  padding-top 12%
.strapline
  background-color #fff
  background-color rgba(255,255,255,0.22)
  opacity 1
  width 100%
  height 40%
  h4
    color black
    font-size 4em
    text-align center
  h2
    color black
    font-family 'proxima_nova_rgbold'
    font-size 2em
    letter-spacing 0.1em
    margin 36px auto 20px
    text-align center
.title-container
  .dash
    border-bottom 3px solid #CCCCCC
    width 30px
.img-scale
  max-width 10%
  min-height 75px
  min-width 75px
.stockickr-at-the-top
  padding-top 80px
  text-align center
  position relative
  font-family 'leagueG'
  color white
  text-transform uppercase
  font-size 400%
  text-shadow .5px 0px .5px #434343
.hasher
  color red
.intro-text
  padding-bottom 30px
  color white  
  margin 60px auto 60px auto
  opacity 0.85
  position relative
  text-align center
  width 100%
  font-size 175%
  text-shadow .5px 0px .5px #434343
.logo
  vertical-align middle
  padding-top 50px
table
  border-collapse collapse
  border-spacing 0
  opacity 1
.featuredText
  height 20%
  left 0
  position relative
  text-align center  
  padding-bottom 200px
  width 100%
  z-index 5
//end of header parallax
//mid-page bleeder content
.bleedout
  min-width 100%
  height 40%
  min-height 250px
  position relative
  font-family 'leagueG'
  text-align center
  color white
  text-transform uppercase
  padding-top 8%
  font-size 500%
  line-height 100px
  vertical-align middle
  text-shadow 7px 7px 10px #434343
.bleedout-white
  width 100%
  height auto
  min-height 350px
  padding-top 10%
  padding-bottom 10%
  position relative
  font-family 'leagueG'
  text-align center
  text-transform uppercase
  font-size 350%
  line-height 80px
  border-radius 25
//NYC parallaxes
.wam
  //background-repeat repeat
  background url('/images/NYCskyline.png') center center fixed
  background-size cover
  width auto
.wam2
  //background-repeat repeat
  background url('/images/NYCskyline.png') center center fixed
  background-size cover
.bleedout2
  width 50%
  height 200px
  position relative
  font-family 'dense-regular-webfont'
  text-align center
  color white
  text-transform uppercase
  padding-top 5%
  font-size 40px
  background-color rgba(255,255,255,0.01)
.bleedout3
  width auto
  height auto
  position relative
  font-family 'dense-regular-webfont'
  text-align center
  color white
  text-transform uppercase
  padding-top 3%
  padding-bottom 3%
  font-size 40px
  overflow scroll

//clickable logos and icons, that also change state on hover
.floating-logo
  background-size cover
  width 59px
  height 59px
  background-image url('data:image/png;base64,')
  background-repeat no-repeat
  &:hover
    cursor pointer
    background-image url('data:image/png;base64,')


